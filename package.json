{
	"name": "@optum/json-schema-editor",
	"version": "2.0.1",
	"description": "JsonSchema Editor React Control",
	"repository": "https://github.com/optum/jsonschema-editor-react",
	"license": "Apache 2.0",
	"engines": {
		"node": ">=10.18.0 <11 || >=12.14.0 <13 || >=13.5.0"
	},
	"source": "src/JsonSchemaEditor/index.ts",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"keywords": [
		"react"
	],
	"scripts": {
		"start": "react-scripts start",
		"build": "microbundle --jsx React.createElement",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "start-storybook -p 6006 -s docs",
		"build-storybook": "rimraf docs && build-storybook --docs -o docs"
	},
	"eslintConfig": {
		"extends": "react-app",
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@chakra-ui/react": "1.6.0",
		"@emotion/react": "^11.0.0",
		"@emotion/styled": "^11.0.0",
		"@hookstate/core": "^3.0.7",
		"framer-motion": "^4.0.0",
		"ramda": "^0.27.1",
		"react-icons": "^3.0.0",
		"typescript": "^3.9.5",
		"use-debounce": "^6.0.1",
		"web-vitals": "^0.2.2"
	},
	"peerDependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^6.2.9",
		"@storybook/addon-console": "^1.2.3",
		"@storybook/addon-essentials": "^6.2.9",
		"@storybook/addon-links": "^6.2.9",
		"@storybook/node-logger": "^6.2.9",
		"@storybook/preset-create-react-app": "^3.1.7",
		"@storybook/react": "^6.2.9",
		"@testing-library/jest-dom": "^5.9.0",
		"@testing-library/react": "^10.2.1",
		"@testing-library/user-event": "^12.0.2",
		"@types/jest": "^25.0.0",
		"@types/node": "^12.0.0",
		"@types/ramda": "^0.27.40",
		"@types/react": "^16.0.0",
		"@types/react-dom": "^16.0.0",
		"microbundle": "^0.13.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-scripts": "4.0.3"
	}
}
